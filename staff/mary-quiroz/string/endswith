delete String.prototype.endsWith
function endswith(string, searchString, position){
    debugger
var stringLength= string.length
var searchStringLength=searchString.length
var initialPositionIndex= stringLength - searchStringLength

if(position){
    stringLength = position
}

for (var i = initialPositionIndex; i < stringLength; i++) {
    var charString = string[i]  // M

    var searchStringIndex = i - searchStringLength

    var charSearchString = searchString[searchStringIndex]
    
    if( charString !== charSearchString){
        return false   
        
    }
    else if(i === stringLength-1 ){
        return true
    }
}
}

var string= "Hola Mundo"
var searchString="Mundo"
var position = 5
endswith(string, searchString, position)

// CASE 1

var s = 'hola mundo'

var result = endsWith(s, 'ndo')

console.log(result)
// true

// CASE 2

var s = 'hola mundo'

var result = endsWith(s, 'dos')

console.log(result)
// false